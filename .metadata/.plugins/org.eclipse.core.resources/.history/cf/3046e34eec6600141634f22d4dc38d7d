package uni;

public class Test {

		   public static void copyArray(int[] x)  
		   {
			int y[] = {1,2,3};		
			x = y;
		   }
		   public static void main (String[] args){
			int a[] = {10,20,30};
			int b[] = {100,200,300};
			copyArray(a);
			System.out.print(a[0]+" "+a[1]+" "+a[2]+" ");
			a = b;
			System.out.println(a[0]+" "+a[1]+" "+a[2]);
		   }
}
