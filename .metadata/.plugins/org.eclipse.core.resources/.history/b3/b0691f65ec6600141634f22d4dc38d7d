package uni;

public class Test {

	public static int[] copyArray(int[] x)  
	   {
		int y[] = new int[3];		
		for (int i=0; i<3; i++) {
			x[i]++;
			y[i] = x[i]*3;
		}
		return y;
	   }

	   public static void main (String[] args){
		int a[] = {10,20,30};
		a = copyArray(a);
		System.out.print(a[0]+" "+a[1]+" "+a[2]+" ");
	   }
}
