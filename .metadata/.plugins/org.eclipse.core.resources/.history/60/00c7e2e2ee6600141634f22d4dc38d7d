package uni;

public class Test {

	public static void main (String[] args)
	   {
	      int[][] m = new int[3][3];

	      for (int i = 0; i < 3; i++) 
	         for (int j = 0; j < 3; j++)
	            if (i % 2 == 0) 
	               m[i][j] = 0;
	            else 
	               m[i][j] = 1;	
	 
	      for (int i = 0; i < 3; i++) 
	      {
	         for (int j = 0; j < 3; j++)
	            System.out.print("   "+ m[i][j]);

	         System.out.println();
	      }	
	   }
}
